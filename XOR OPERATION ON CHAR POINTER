#include <stdio.h>
#include <string.h>

int main() {
    char *str = "Hello World";
    char str1[12];
    int i, len;
    
    len = strlen(str);
    
    for(i = 0; i < len; i++) {
        str1[i] = str[i] ^ 0;  // XOR with 0 leaves the character unchanged
        printf("%c", str1[i]);
    }
    
    str1[i] = '\0';  // Null-terminate the string
    printf("\n");
    printf("%s\n", str1);
    
    return 0;
}

// output 
Hello World
Hello World
